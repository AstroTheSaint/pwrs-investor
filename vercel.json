{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/investor-pitch",
      "dest": "/investor-pitch/index.html"
    },
    {
      "src": "/investor-pitch/(.*)",
      "dest": "/investor-pitch/$1"
    }
  ]
} 